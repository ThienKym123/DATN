    "start": "PORT=5000 GENERATE_SOURCEMAP=false react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
